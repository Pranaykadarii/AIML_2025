    {
      "cell_type": "code",
      "source": [
        "#Step 2: Detect outliers using z score method\n",
        "z_scores=stats.zscore(df['Value'])\n",
        "outliers_zscore=df[np.abs(z_scores)>3]"
      ],
      "metadata": {
        "id": "xjpHSyO7gkR-"
      },
      "execution_count": 9,
